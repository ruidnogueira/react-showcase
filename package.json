{
  "name": "react-showcase",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://ruidnogueira.github.io/react-showcase/",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "e2e": "cross-env VITE_E2E=true playwright test",
    "e2e:debug": "cross-env PWDEBUG=1 VITE_E2E=true playwright test",
    "prepare": "husky install",
    "lint": "npm run lint:lslint && npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:lslint": "ls-lint",
    "lint:eslint": "eslint --max-warnings=0 src/**/*.{ts,tsx}",
    "lint:stylelint": "stylelint src/**/*.{css,scss}",
    "lint:prettier": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic --exit-zero-on-changes --skip 'renovate/**'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@radix-ui/react-select": "^1.0.0",
    "@radix-ui/react-slot": "1.0.0",
    "@radix-ui/react-tooltip": "1.0.0",
    "@radix-ui/react-visually-hidden": "1.0.0",
    "clsx": "1.2.1",
    "date-fns": "2.29.1",
    "i18next": "21.8.14",
    "i18next-browser-languagedetector": "6.1.4",
    "i18next-http-backend": "1.4.1",
    "modern-normalize": "1.1.0",
    "phosphor-react": "1.4.1",
    "rambda": "7.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-helmet-async": "1.3.0",
    "react-i18next": "11.18.1",
    "react-router-dom": "6.3.0",
    "url-join": "5.0.0",
    "workbox-core": "6.5.3",
    "workbox-expiration": "6.5.3",
    "workbox-precaching": "6.5.3",
    "workbox-routing": "6.5.3",
    "workbox-strategies": "6.5.3",
    "workbox-window": "6.5.3"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@ls-lint/ls-lint": "1.11.2",
    "@playwright/test": "1.24.0",
    "@storybook/addon-a11y": "6.5.9",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-interactions": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/builder-vite": "0.2.2",
    "@storybook/react": "6.5.9",
    "@storybook/testing-library": "0.0.13",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.3.0",
    "@types/jest-axe": "3.5.4",
    "@types/picomatch": "2.3.0",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "@vitejs/plugin-react": "2.0.0",
    "autoprefixer": "10.4.7",
    "babel-loader": "8.2.5",
    "c8": "7.12.0",
    "chromatic": "6.7.1",
    "cross-env": "7.0.3",
    "dotenv": "16.0.1",
    "esbuild-register": "3.3.3",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.3.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.6.0",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-playwright": "0.10.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.1",
    "eslint-plugin-testing-library": "5.5.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "gh-pages": "4.0.0",
    "husky": "8.0.1",
    "jest-axe": "6.0.0",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.3",
    "msw": "0.44.2",
    "msw-storybook-addon": "1.6.3",
    "picomatch": "2.3.1",
    "playwright": "1.24.0",
    "prettier": "2.7.1",
    "rollup-plugin-visualizer": "5.7.1",
    "sass": "1.54.0",
    "stylelint": "14.9.1",
    "stylelint-config-css-modules": "4.1.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.3.0",
    "typescript": "4.7.4",
    "utility-types": "3.10.0",
    "vite": "3.0.3",
    "vite-plugin-pwa": "0.12.3",
    "vite-plugin-svgr": "2.2.1",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.19.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
