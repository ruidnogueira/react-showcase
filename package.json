{
  "name": "react-showcase",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://ruidnogueira.github.io/react-showcase/",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "e2e": "cross-env VITE_E2E=true playwright test",
    "e2e:debug": "cross-env PWDEBUG=1 VITE_E2E=true playwright test",
    "prepare": "husky install",
    "lint": "npm run lint:lslint && npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier && npm run lint:dependencies",
    "lint:lslint": "ls-lint",
    "lint:eslint": "eslint --max-warnings=0 src/**/*.{ts,tsx}",
    "lint:stylelint": "stylelint src/**/*.{css,scss}",
    "lint:prettier": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
    "lint:dependencies": "depcruise src --config .dependency-cruiser.cjs",
    "graph:modules": "depcruise src --config graphs/.modules.cjs -T dot | dot -T svg | depcruise-wrap-stream-in-html > graphs/results/modules-graph.html",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic --exit-zero-on-changes --skip 'renovate/**'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@radix-ui/react-select": "1.1.2",
    "@radix-ui/react-slot": "1.0.1",
    "@radix-ui/react-tooltip": "1.0.5",
    "@radix-ui/react-visually-hidden": "1.0.2",
    "axios": "1.2.2",
    "clsx": "1.2.1",
    "date-fns": "2.29.3",
    "fp-ts": "2.13.1",
    "i18next": "22.0.6",
    "i18next-browser-languagedetector": "7.0.1",
    "i18next-http-backend": "2.0.2",
    "modern-normalize": "1.1.0",
    "phosphor-react": "1.4.1",
    "rambda": "7.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-helmet-async": "1.3.0",
    "react-i18next": "12.0.0",
    "react-idle-timer": "5.4.2",
    "react-router-dom": "6.4.4",
    "react-transition-group": "4.4.5",
    "url-join": "5.0.0",
    "uuid": "9.0.0",
    "workbox-core": "6.5.4",
    "workbox-expiration": "6.5.4",
    "workbox-precaching": "6.5.4",
    "workbox-routing": "6.5.4",
    "workbox-strategies": "6.5.4",
    "workbox-window": "6.5.4"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@ls-lint/ls-lint": "1.11.2",
    "@mswjs/data": "0.11.2",
    "@playwright/test": "1.28.1",
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/builder-vite": "0.2.7",
    "@storybook/react": "6.5.16",
    "@storybook/testing-library": "0.0.13",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/chance": "1.1.3",
    "@types/jest-axe": "3.5.5",
    "@types/picomatch": "2.3.0",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@types/react-transition-group": "4.4.5",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "@vitejs/plugin-react": "2.2.0",
    "@vitest/coverage-istanbul": "0.25.3",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.2",
    "chance": "1.1.11",
    "chromatic": "6.11.4",
    "cross-env": "7.0.3",
    "dependency-cruiser": "12.1.0",
    "dotenv": "16.0.3",
    "esbuild-register": "3.4.2",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.7",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-playwright": "0.11.2",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.11",
    "eslint-plugin-testing-library": "5.9.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "gh-pages": "4.0.0",
    "husky": "8.0.3",
    "jest-axe": "7.0.1",
    "jsdom": "20.0.3",
    "lint-staged": "13.0.4",
    "msw": "0.49.1",
    "msw-storybook-addon": "1.6.3",
    "picomatch": "2.3.1",
    "playwright": "1.28.1",
    "prettier": "2.8.1",
    "rollup-plugin-visualizer": "5.8.3",
    "sass": "1.56.1",
    "stylelint": "14.15.0",
    "stylelint-config-css-modules": "4.1.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.3.0",
    "type-fest": "3.3.0",
    "typescript": "4.9.5",
    "vite": "3.2.6",
    "vite-plugin-pwa": "0.13.3",
    "vite-plugin-svgr": "2.2.2",
    "vite-tsconfig-paths": "3.6.0",
    "vitest": "0.25.3"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
